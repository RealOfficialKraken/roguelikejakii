)
(set! in-updateboard? #f)
(set! camera-unlocked #t)
(set! ub-select 1)
(set! ub-npc-kill #t)
)

(when (= ub-npc-kill #t)
(+! ub-shopi 1)
(when (= ub-shopi 40)
